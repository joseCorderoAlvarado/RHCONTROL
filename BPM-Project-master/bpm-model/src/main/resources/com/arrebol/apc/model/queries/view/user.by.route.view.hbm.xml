<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    
    <query name="findAllUsersByRou">
        <![CDATA[
            SELECT 
                t
            FROM GeneralBoxView t
            WHERE office = :office                
        ]]>
    </query>

</hibernate-mapping>
